<div class="container paddingbottom20 paddingtop20">
    <div class="row">
        <div class="  col-md-8 col-md-offset-2 ">
            <h3 class="page-header ">Add new listing</h3>
            <form class="form-horizontal well noradiusborder" name="addlistingForm" novalidate>
                <fieldset>


                    <!-- Text input  -->
                    <div class="form-group form-group-lg margintop20">
                    <label for="tender-title" class="control-label col-sm-4">Title</label>
                    <div class="col-sm-7">
                        <input type="text" class="form-control" id="tender-title" placeholder="Tender title" required="">
                        
                    </div>
                    </div>
                    <!-- Text input  -->
                    <div class="form-group form-group-lg">
                    <label for="tender-caller" class="control-label col-sm-4">Caller</label>
                    <div class="col-sm-7">
                        <input type="text" class="form-control" id="tender-caller" placeholder="Name and address" required="">
                        
                    </div>
                    </div>
                    <!-- Text input  -->
                    <div class="form-group form-group-lg">
                    <label for="pub-date" class="control-label col-sm-4">Publication Date</label>
                    <div class="col-sm-7">
                        <input type="date" class="form-control" id="pub-date" placeholder="eg. 2015-07-28" required="">
                        
                    </div>
                    </div>
                    <!-- Text input  -->
                    <div class="form-group form-group-lg">
                    <label for="pub-daily" class="control-label col-sm-4">Publication Daily</label>
                    <div class="col-sm-7">
                        <input type="text" class="form-control" id="pub-daily" placeholder="Published newspaper" required="">
                        
                    </div>
                    </div>
                    <!-- Text input  -->
                    <div class="form-group form-group-lg">
                    <label for="sub-date" class="control-label col-sm-4">Submission Date</label>
                    <div class="col-sm-7">
                        <input type="date" class="form-control" id="sub-date" placeholder="eg. 2015-07-28" required="">
                        
                    </div>
                    </div>
                    
                    <!-- Input With Combobox  -->
                    <div class="form-group form-group-lg">
                    <label class="control-label col-sm-4" for="type">Type</label>
                    <div class="controls text-right col-sm-7">
                        <select class="form-control">
                            <option value="Tender">Tender</option>
                            <option value="Quotation">Quotation</option>
                            <option value="Proposal">Proposal</option>
                            <option value="EOI">EOI</option>
                            <option value="Other">Other</option>
                        </select>
                    </div>
                    </div>
                    
                    <!-- Textarea  -->
                    <div class="form-group form-group-lg">
                    <label class="control-label col-sm-4" for="tags">Tags</label>
                    <div class="col-sm-7">
                        <textarea class="form-control" id="tags" name="tags" rows="5"></textarea>
                        
                    </div>
                    </div>

                    <!-- Button  -->
                    <div class="form-group">
                    <label class="control-label col-sm-4" for="submit-tender"></label>
                    <div class="text-right col-sm-7">
                        <button type="submit" id="submit-tender" name="submit-tender" class="btn btn-primary btn-lg" aria-label=""><span class="glyphicon glyphicon-export" aria-hidden="true"></span> Submit</button>
                        
                    </div>
                    </div>


                </fieldset>
            </form>
        </div>
    </div>
</div>